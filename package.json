{
  "name": "project-setup",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "tsc --noEmit && eslint \"*/**/*.{js,ts,tsx,jsx}\" --quiet --fix",
    "format": "prettier --write .",
    "api:generate": "npx @rtk-query/codegen-openapi openapi-config.ts",
    "api:delete": "cross-env DELETE_FILE=src/store/queries.ts npm run delete-file",
    "delete-file": "npm run delete-file:mac || npm run delete-file:win",
    "delete-file:mac": "rm $DELETE_FILE",
    "delete-file:win": "del %DELETE_FILE%"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.6",
    "@mui/material": "^5.14.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "20.4.6",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "moment": "^2.29.4",
    "next": "13.4.12",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-intersection-observer": "^9.5.2",
    "react-redux": "^8.1.2",
    "react-scroll": "^1.8.9",
    "typescript": "5.1.6",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@rtk-query/codegen-openapi": "^1.0.0",
    "@types/react-scroll": "^1.8.7",
    "@types/redux-logger": "^3.0.9",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "redux-logger": "^3.0.6",
    "ts-node": "^10.9.1"
  }
}